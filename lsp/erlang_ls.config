deps_dirs:
  - "_build/*/lib/*"
  - "_build/*/lib/*/include"
  - "_build/*/lib/*/include/*"
  - "_build/*/checkouts/*"
  - "_build/*/checkouts/*/include"
  - "_build/*/checkouts/*/include/*"
  - "lib/*"
diagnostics:
  enabled:
    - crossref
  disabled:
    - bound_var_in_pattern
apps_dirs:
  - "apps/*"
  - "_build/*/lib/*"
  - "_build/*/lib/*/include"
  - "_build/*/lib/*/include/*"
  - "_build/*/checkouts/*"
  - "_build/*/checkouts/*/include"
  - "_build/*/checkouts/*/include/*"
  - "lib/*"
  - "."
include_dirs:
  - "apps"
  - "apps/*/include"
  - "include"
  - "include/*"
  - "."
  - ".."
  - "src"
  - "_build/*/lib"
  - "_build/*/lib/*/include"
  - "_build/*/lib/*/include/*"
  - "_build/*/checkouts"
  - "_build/*/checkouts/*/include"
  - "_build/*/checkouts/*/include/*"
  - "extras"
  - "test"
  - "test/*_data"
lenses:
  enabled:
    - suggest-spec
    - function-references
    - ct-run-test
providers:
  enabled:
    - signature-help

